{
	"name" : "bideogemu/mpdf-bundle",
	"description" : "Mpdf for Symfony 5+",
	"keywords" : [
		"mpdf",
		"symfony",
		"bideogemu"
	],
	"type" : "symfony-bundle",
    "homepage": "https://github.com/BideoGemu/mpdf-bundle",
	"license" : "MIT",
    "authors": [
        {
            "name": "Bideo Gemu",
            "homepage": "https://bideogemu.com",
            "email": "contact@bideogemu.com"
        }
    ],
	"config" : {
		"bin-dir" : "bin",
		"component-dir" : "vendor",
		"preferred-install" : {
			"*" : "dist"
		},
		"process-timeout" : 1200,
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"BideoGemu\\MpdfBundle\\" : "src"
		}
	},
	"require" : {
		"php": ">=8.0",
		"mpdf/mpdf": "^8.0.10",
		"symfony/config": "^5.4 || ^6.0 || ^7.0",
		"symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
		"symfony/http-kernel": "^5.4 || ^6.0 || ^7.0"
	},
    "prefer-stable": true
}
